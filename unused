d3.select("finalGrade")
    .on("click", function(){
        console.log("clicked");
        subjects.sort(function(a,b){
            if (a.final > b.final) {
                return 1;
            } else if (a.final < b.final){
                return -1;
            } else {
                return 0;
            }
        });
        clearTable();
        drawTable(penguins);
    });
};
